<main class="landing-content">
    <h1>Tech Blog</h1>
    
    <ul>
        {{#each posts as |post|}}
        <li>
            <div><h2>{{post.title}}</h2></div>
            <div>{{post.content}}</div>
            <div>{{post.User.firstName}} {{post.User.lastName}} {{format_date post.createDate}}</div>
        </li>
        {{/each}}
    </ul>
</main>